extend input GraphCommerceConfig {
  """
  OCI settings config
  """
  ociSettings: MagentoOciSettingsConfig
}

"""
MagentoOciSettingsConfig to allow enabling certain aspects of the OCI settings
"""
input MagentoOciSettingsConfig {
  """
  Enable OCI sessions for GraphCommerce
  """
  enabled: Boolean

  """
  OCI endpoint URL (required)
  """
  endpoint: String
}
