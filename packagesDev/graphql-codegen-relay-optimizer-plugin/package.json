{
  "name": "@reachdigital/graphql-codegen-relay-optimizer-plugin",
  "version": "0.1.0",
  "description": "GraphQL Code Generator plugin for optimizing your GraphQL queries relay style.",
  "license": "MIT",
  "main": "dist/main/index.js",
  "module": "dist/module/index.js",
  "keywords": [
    "graphql",
    "codegen",
    "graphql-codegen",
    "plugin",
    "relay"
  ],
  "peerDependencies": {
    "graphql": "14.x.x || 15.x.x"
  },
  "dependencies": {
    "@graphql-codegen/plugin-helpers": "1.18.8",
    "relay-compiler": "11.0.2"
  },
  "devDependencies": {
    "@graphql-codegen/testing": "1.17.7",
    "@types/jest": "26.0.24",
    "@types/relay-compiler": "8.0.1",
    "graphql": "15.5.1",
    "jest": "27.0.6",
    "ts-jest": "27.0.3",
    "typescript": "^4.3.5"
  },
  "scripts": {
    "test": "jest",
    "dev": "tsc --preserveWatchOutput --watch --sourceMap --outDir dist/main",
    "build:module": "tsc --target es2017 --outDir dist/module",
    "build:main": "tsc --target es5 --outDir dist/main",
    "build": "yarn build:module && yarn build:main"
  },
  "files": [
    "dist/**/*",
    "LICENSE",
    "README.md"
  ]
}
