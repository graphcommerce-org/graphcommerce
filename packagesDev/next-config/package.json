      "types": "./src/generated/configValues/index.ts",
      "default": "./dist/generated/configValues/index.js"
    }
  },
  "typesVersions": {
    "*": {
      "config": [
        "src/generated/configValues/index.ts"
      ]
    }
  },
  "scripts": {
    "dev": "pkgroll --clean-dist --watch",
    "build": "pkgroll --clean-dist",
    "prepack": "pkgroll --clean-dist"
  },
  "dependencies": {
    "@graphql-codegen/cli": "5.0.7",
    "@swc/core": "1.13.5",
    "@swc/wasm-web": "^1.13.5",
    "@types/circular-dependency-plugin": "^5.0.8",
    "@types/js-yaml": "^4.0.9",
    "@types/lodash": "^4.17.20",
    "babel-plugin-macros": "^3.1.0",
    "chalk": "^4.1.2",
    "circular-dependency-plugin": "^5.2.2",
    "cosmiconfig": "^8.3.6",
    "fast-glob": "^3.3.3",
    "glob": "^10.4.5",
    "graphql": "^16.11.0",
    "inspectpack": "^4.7.1",
    "js-yaml": "^4.1.0",
    "js-yaml-loader": "^1.2.2",
    "lodash": "^4.17.21",
    "react": "^19.1.1",
    "typescript": "5.9.2",
    "webpack": "^5.97.1",
    "znv": "^0.4.0",
    "zod": "^3.24.1"
  },
  "peerDependencies": {
    "@graphcommerce/prettier-config-pwa": "^9.1.0-canary.55",
    "@lingui/loader": "*",
    "@lingui/macro": "*",
    "@lingui/react": "*",
    "dotenv": "^16",
    "next": "*",
    "prettier": "^3"
  },
  "prettier": "@graphcommerce/prettier-config-pwa",
  "eslint": {
    "extends": "@graphcommerce/eslint-config-pwa"
  },
  "eslintConfig": {
    "extends": "@graphcommerce/eslint-config-pwa",
    "parserOptions": {
      "project": "./tsconfig.json"
    }
  },
  "devDependencies": {
    "pkgroll": "^2.15.3"
  }
}
