{
  "name": "GraphCommerce",
  "id": "?homescreen=1",
  "short_name": "GraphCommerce",
  "description": "GraphCommerceÂ® is a headless e-commerce frontend for Magento.",
  "display": "standalone",
  "theme_color": "#000000",
  "background_color": "#FFFFFF",
  "start_url": "/",
  "launch_handler": {
    "client_mode": ["navigate-existing", "auto"]
  },
  "orientation": "portrait-primary",
  "icons": [
    {
      "src": "/manifest/favicon-192.png",
      "sizes": "192x192",
