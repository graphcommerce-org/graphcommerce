    {
      "src": "/manifest/favicon-512.png",
      "sizes": "512x512",
      "type": "image/png",
      "purpose": "any"
    }
  ],
  "screenshots": [
    {
      "src": "/manifest/screenshots/home.png",
      "sizes": "720x1280",
      "type": "image/png",
      "form_factor": "narrow",
