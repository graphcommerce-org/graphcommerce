    },
    {
      "src": "/manifest/screenshots/nav.png",
      "sizes": "720x1280",
      "type": "image/png",
      "form_factor": "narrow",
      "label": "Navigation"
    },
    {
      "src": "/manifest/screenshots/upsell.png",
      "sizes": "720x1280",
      "type": "image/png",
      "form_factor": "Upsell",
      "label": "Navigation"
    }
  ],
  "dir": "ltr",
  "categories": ["developer tools", "shopping"],
  "lang": "nl",
  "prefer_related_applications": false,
  "scope": "https://graphcommerce.vercel.app/",
  "scope_extensions": [{ "origin": "*.vercel.app" }],
