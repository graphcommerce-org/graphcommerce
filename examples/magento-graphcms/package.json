    "@graphcommerce/graphcms-ui": "3.1.1",
    "@graphcommerce/graphql": "3.4.8",
    "@graphcommerce/graphql-mesh": "4.2.0",
    "@graphcommerce/image": "3.1.9",
    "@graphcommerce/lingui-next": "2.2.1",
    "@graphcommerce/magento-cart": "4.8.7",
