    "@graphcommerce/next-ui": "^4.2.5",
    "@graphql-codegen/cli": "^2.6.2",
    "@graphql-mesh/cli": "^0.59.0",
    "@lingui/macro": "^3.13.2",
    "@mui/lab": "^5.0.0-alpha.72",
    "@mui/material": "^5.5.0",
    "concurrently": "^7.0.0",
    "dotenv": "^16.0.0",
    "framer-motion": "^6.2.8",
    "next": "^12.1.0",
    "next-pwa": "^5.4.5",
