    "@playwright/test": "^1.17.1",
    "apollo3-cache-persist": "^0.13.0",
    "clsx": "^1.1.1",
    "concurrently": "^6.5.0",
    "decode-html": "^2.0.0",
    "dotenv": "^10.0.0",
    "framer-motion": "^5.5.5",
    "graphql": "^16.2.0",
    "graphql-tag": "^2.12.6",
    "next": "^12.0.7",
    "next-pwa": "^5.4.4",
    "next-sitemap": "^1.6.203",
