    "@graphcommerce/googletagmanager": "2.0.7",
    "@graphcommerce/graphcms-ui": "3.0.14",
    "@graphcommerce/graphql": "3.1.1",
    "@graphcommerce/graphql-mesh": "4.1.2",
    "@graphcommerce/image": "3.1.5",
    "@graphcommerce/lingui-next": "2.1.7",
