{
  "name": "@graphcommerce/magento-graphcms",
  "version": "2.145.0",
  "private": true,
  "sideEffects": false,
  "engines": {
    "node": "14.x"
  },
  "scripts": {
    "dev": "yarn concurrently -k -n codegen,next 'yarn codegen-base -w' 'yarn next dev'",
    "codegen": "yarn mesh build --require dotenv/config && NEXT_PUBLIC_GRAPHQL_ENDPOINT=.mesh/schema.graphql yarn codegen-base",
    "build": "yarn codegen && yarn next build && yarn next-sitemap",
    "start": "next start",
    "tsc:lint": "yarn tsc --noEmit -p .",
    "lingui": "NODE_ENV=development lingui extract",
    "codegen-base": "NODE_TLS_REJECT_UNAUTHORIZED=0 CHOKIDAR_USEPOLLING=0 yarn graphql-codegen --require dotenv/config -c codegen.mono.yml",
    "prepack": "concurrently 'yarn prepack:1' 'yarn prepack:2' 'yarn prepack:3'",
    "prepack:1": "yarn workspace @graphcommerce/next-config prepack",
    "prepack:2": "yarn workspace @graphcommerce/graphql-codegen-near-operation-file prepack",
    "prepack:3": "yarn workspace @graphcommerce/graphql-codegen-relay-optimizer-plugin prepack",
    "postinstall": "yarn prepack"
  },
  "dependencies": {
    "@apollo/client": "^3.5.6",
    "@graphcommerce/browserslist-config-pwa": "^3.0.3",
    "@graphcommerce/eslint-config-pwa": "^3.1.10",
    "@graphcommerce/framer-next-pages": "^2.109.2",
    "@graphcommerce/googleanalytics": "^1.2.1",
    "@graphcommerce/googlerecaptcha": "^1.1.9",
    "@graphcommerce/googletagmanager": "^1.0.9",
    "@graphcommerce/graphcms-ui": "^2.106.37",
    "@graphcommerce/graphql": "^2.105.13",
    "@graphcommerce/graphql-mesh": "^3.2.11",
    "@graphcommerce/image": "^2.105.13",
    "@graphcommerce/lingui-next": "^1.3.1",
    "@graphcommerce/magento-cart": "^3.10.1",
    "@graphcommerce/magento-cart-billing-address": "^2.107.45",
    "@graphcommerce/magento-cart-checkout": "^2.107.46",
    "@graphcommerce/magento-cart-coupon": "^2.109.40",
    "@graphcommerce/magento-cart-email": "^2.107.41",
    "@graphcommerce/magento-cart-items": "^2.110.46",
    "@graphcommerce/magento-cart-payment-method": "^2.111.45",
    "@graphcommerce/magento-cart-shipping-address": "^2.106.45",
    "@graphcommerce/magento-cart-shipping-method": "^2.106.45",
    "@graphcommerce/magento-category": "^3.5.41",
    "@graphcommerce/magento-cms": "^3.2.42",
    "@graphcommerce/magento-customer": "^3.6.42",
    "@graphcommerce/magento-customer-account": "^2.105.42",
    "@graphcommerce/magento-customer-order": "^2.105.42",
    "@graphcommerce/magento-graphql": "^2.104.13",
    "@graphcommerce/magento-newsletter": "^1.1.45",
    "@graphcommerce/magento-payment-included": "^2.105.46",
    "@graphcommerce/magento-product": "^3.8.11",
    "@graphcommerce/magento-product-bundle": "^3.2.46",
    "@graphcommerce/magento-product-configurable": "^3.6.41",
    "@graphcommerce/magento-product-downloadable": "^3.2.46",
    "@graphcommerce/magento-product-grouped": "^2.105.46",
    "@graphcommerce/magento-product-simple": "^3.2.46",
    "@graphcommerce/magento-product-virtual": "^3.2.46",
