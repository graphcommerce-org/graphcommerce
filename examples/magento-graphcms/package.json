    "@graphcommerce/graphql-codegen-relay-optimizer-plugin": "^2.102.9",
    "@graphcommerce/graphql-mesh": "^3.2.1",
    "@graphcommerce/image": "^2.104.13",
    "@graphcommerce/magento-cart": "^3.2.6",
    "@graphcommerce/magento-cart-billing-address": "^2.105.3",
    "@graphcommerce/magento-cart-checkout": "^2.105.3",
    "@graphcommerce/magento-cart-coupon": "^2.105.35",
    "@graphcommerce/magento-cart-email": "^2.104.37",
    "@graphcommerce/magento-cart-items": "^2.105.7",
    "@graphcommerce/magento-cart-payment-method": "^2.107.6",
    "@graphcommerce/magento-cart-shipping-address": "^2.104.3",
    "@graphcommerce/magento-cart-shipping-method": "^2.103.36",
