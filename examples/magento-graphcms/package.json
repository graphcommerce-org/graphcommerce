{
  "name": "@graphcommerce/magento-graphcms",
  "homepage": "https://www.graphcommerce.org/",
  "repository": "github:graphcommerce-org/graphcommerce",
  "version": "3.27.4",
  "private": true,
  "sideEffects": false,
  "engines": {
    "node": ">=14.18.3"
  },
  "scripts": {
    "dev": "concurrently -k -n codegen,next \"yarn:graphql-codegen -c ../../scripts/codegen.magento-graphcms.yml -w\" \"yarn:next dev\"",
    "codegen": "mesh build && graphql-codegen -c ../../scripts/codegen.magento-graphcms.yml",
    "build": "mesh build && graphql-codegen -c ../../scripts/codegen.magento-graphcms.yml && next build && next-sitemap",
    "start": "next start",
    "tsc:lint": "tsc --noEmit -p .",
    "lingui": "cross-env NEXT_PUBLIC_LOCALE_STORES='{\"en\": \"en\", \"nl\": \"nl\", \"fr\": \"fr\", \"nl\": \"nl\", \"de\": \"de\", \"es\": \"es\", \"it\": \"it\"}' NODE_ENV=development lingui extract --clean",
    "postinstall": "cd ../.. && yarn postinstall"
  },
  "scripts_local": {
    "dev": "concurrently -k -n codegen,next 'graphql-codegen -w' 'next dev'",
    "codegen": "mesh build && graphql-codegen",
    "build": "mesh build && graphql-codegen && next build && next-sitemap",
    "start": "next start",
    "tsc:lint": "tsc --noEmit -p .",
    "lingui": "cross-env NODE_ENV=development lingui extract",
    "postinstall": "patch-package"
  },
  "dependencies": {
    "@graphcommerce/cli": "1.0.10",
    "@graphcommerce/ecommerce-ui": "1.5.3",
    "@graphcommerce/framer-next-pages": "3.3.1",
