    "build": "yarn build:next-config && yarn build:near-operation && yarn build:relay-optimizer && yarn codegen && next build && yarn next-sitemap",
    "start": "next start",
    "postinstall": "patch-typed-document-node && cd ../.. && yarn postinstall"
