{
  "name": "project-reachdigital.nl",
  "homepage": "https://project-reachdigital-nl.now.sh/",
  "version": "1.20.1",
  "private": true,
  "engines": {
    "node": "12.x"
  },
  "scripts": {
    "dev": "concurrently yarn:dev-* --kill-others",
    "dev-next": "next dev",
    "codegen": "TS_NODE_COMPILER_OPTIONS='{\"module\":\"commonjs\"}' graphql-codegen",
    "dev-codegen": "TS_NODE_COMPILER_OPTIONS='{\"module\":\"commonjs\"}' graphql-codegen -w",
    "build": "TS_NODE_COMPILER_OPTIONS='{\"module\":\"commonjs\"}' graphql-codegen && next build",
    "start": "next start",
    "eslint:lint": "eslint --ignore-path .gitignore '**/{*.js,*.ts,*.tsx}'",
    "eslint:fix": "eslint --fix --ignore-path .gitignore '**/{*.js,*.ts,*.tsx}'",
    "prettier:fix": "yarn prettier --ignore-path .gitignore --write '**/*.{ts,tsx,js,css,html}'",
    "mesh": "node -r dotenv/config node_modules/.bin/mesh serve"
  },
  "sideEffects": false,
  "dependencies": {
    "@adobe/apollo-link-mutation-queue": "^1.0.2",
    "@apollo/client": "^3.1.1",
    "@graphql-codegen/import-types-preset": "^1.17.6",
    "@graphql-mesh/cli": "^0.2.15",
    "@graphql-mesh/graphql": "^0.2.15",
    "@graphql-mesh/merger-stitching": "^0.2.15",
    "@graphql-mesh/runtime": "^0.2.15",
    "@graphql-mesh/transform-filter-schema": "^0.2.15",
    "@graphql-mesh/transform-snapshot": "^0.2.15",
    "@material-ui/core": "^4.11.0",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/lab": "^4.0.0-alpha.56",
    "@material-ui/styles": "^4.10.0",
    "@n1ru4l/graphql-codegen-relay-optimizer-plugin": "^5.0.0-alpha.0",
    "@next/bundle-analyzer": "^9.5.1",
    "apollo-cache-persist": "^0.1.1",
    "apollo-server-micro": "^2.16.1",
    "clone-deep": "^4.0.1",
    "clsx": "^1.1.1",
    "dotenv": "^8.2.0",
    "framer-motion": "^2.3.0",
    "graphql": "^15.3.0",
    "graphql-tag": "^2.11.0",
    "intl": "^1.2.5",
    "next": "9.5.1",
    "next-images": "^1.4.1",
    "next-pwa": "^3.1.1",
    "next-transpile-modules": "^4.0.2",
    "now": "^19.2.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-hook-form": "^5.7.1",
    "react-schemaorg": "^1.2.0",
    "react-spring": "^8.0.27",
    "resize-observer": "^1.0.0",
    "schema-dts": "^0.6.0",
    "sitemap": "^6.2.0",
    "ts-node": "^8.10.2",
    "tsconfig-paths": "^3.9.0",
    "type-fest": "^0.16.0",
    "use-resize-observer": "^6.1.0"
  },
  "devDependencies": {
    "@babel/compat-data": "^7.10.5",
    "@babel/core": "^7.10.5",
    "@commitlint/cli": "^9.1.1",
    "@commitlint/config-conventional": "^9.1.1",
    "@graphql-codegen/cli": "^1.17.6",
    "@graphql-codegen/fragment-matcher": "1.17.6",
    "@graphql-codegen/introspection": "1.17.6",
    "@graphql-codegen/schema-ast": "^1.17.6",
    "@graphql-codegen/typescript": "1.17.6",
    "@graphql-codegen/typescript-document-nodes": "1.17.6",
    "@graphql-codegen/typescript-operations": "1.17.6",
    "@graphql-codegen/typescript-react-apollo": "1.17.6",
    "@semantic-release/git": "^9.0.0",
    "@types/clone-deep": "^4.0.1",
    "@types/lodash": "^4.14.158",
    "@types/node": "^14.0.27",
    "@types/react": "^16.9.43",
    "@types/resize-observer-browser": "^0.1.3",
    "@types/use-resize-observer": "^6.0.0",
    "@types/webpack": "^4.41.21",
    "@typescript-eslint/eslint-plugin": "^3.7.1",
    "@typescript-eslint/parser": "^3.7.1",
    "concurrently": "^5.2.0",
    "eslint": "^7.5.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-airbnb-typescript": "^9.0.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-import-resolver-webpack": "^0.12.2",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-react": "^7.20.5",
    "eslint-plugin-react-hooks": "^4.0.8",
    "husky": "^4.2.5",
    "prettier": "^2.0.5",
    "pretty-quick": "^2.0.1",
    "semantic-release": "^17.1.1",
    "semantic-release-slack-bot": "^1.6.2",
    "typescript": "^3.9.7",
    "webpack": "^4.44.1"
  },
  "prettier": {
    "trailingComma": "all",
    "tabWidth": 2,
    "semi": false,
    "singleQuote": true,
    "jsxSingleQuote": true,
    "printWidth": 100,
    "proseWrap": "always",
    "overrides": [
      {
        "files": "*.md",
        "options": {
          "printWidth": 80
        }
      }
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "pretty-quick --staged"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ho-nl/m2-pwa.git"
  },
  "release": {
    "plugins": [
      "@semantic-release/commit-analyzer",
      [
        "@semantic-release/release-notes-generator",
        {
          "preset": "conventionalcommits",
          "presetConfig": {
            "types": [
              {
                "type": "docs",
                "section": "Documentation"
              },
              {
                "type": "feat",
                "section": "Features"
              },
              {
                "type": "fix",
                "section": "Fixes"
              },
              {
                "type": "perf",
                "section": "Performance Improvements"
              },
              {
                "type": "build",
                "section": "Build"
              },
              {
                "type": "refactor",
                "section": "Refactor"
              }
            ]
          }
        }
      ],
      "@semantic-release/npm",
      "@semantic-release/git",
      "@semantic-release/github",
      [
        "semantic-release-slack-bot",
        {
          "notifyOnSuccess": true,
          "notifyOnFail": true,
          "markdownReleaseNotes": true,
          "onSuccessTemplate": {
            "blocks": [
              {
                "type": "section",
                "text": {
                  "type": "mrkdwn",
                  "text": "*New release for $repo_url ðŸŽ‰*\n$release_notes"
                }
              }
            ]
          }
        }
      ]
    ],
    "branches": [
      {
        "name": "master"
      }
    ]
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "publishConfig": {
    "access": "restricted"
  }
}
