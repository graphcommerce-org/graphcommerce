{
  "name": "project-reachdigital.nl",
  "homepage": "https://project-reachdigital-nl.now.sh/",
  "version": "1.11.0",
  "private": true,
  "engines": {
    "node": "12.x"
  },
  "browser": {
    "@magento/venia-drivers": "shop/driver"
  },
  "scripts": {
    "dev": "concurrently yarn:dev-* --kill-others",
    "dev-next": "next dev",
    "codegen": "TS_NODE_COMPILER_OPTIONS='{\"module\":\"commonjs\"}' graphql-codegen",
    "dev-codegen": "TS_NODE_COMPILER_OPTIONS='{\"module\":\"commonjs\"}' graphql-codegen -w",
    "DISABLEDdev-storybook": "start-storybook --quiet -p 6006",
    "build": "TS_NODE_COMPILER_OPTIONS='{\"module\":\"commonjs\"}' graphql-codegen && next build",
    "start": "next start",
    "eslint:lint": "eslint --ignore-path .gitignore '**/{*.js,*.ts,*.tsx}'",
    "prettier:fix": "yarn prettier --ignore-path .gitignore --write '**/*.{ts,tsx,js,css,html}'",
    "mesh": "node -r dotenv/config node_modules/.bin/mesh serve"
  },
  "sideEffects": false,
  "dependencies": {
    "@adobe/apollo-link-mutation-queue": "^1.0.1",
    "@apollo/react-common": "^3.1.4",
    "@apollo/react-hooks": "^3.1.5",
    "@apollo/react-ssr": "^3.1.5",
    "@graphql-mesh/cli": "^0.2.12",
    "@graphql-mesh/graphql": "^0.2.12",
    "@graphql-mesh/merger-stitching": "^0.2.12",
    "@graphql-mesh/runtime": "^0.2.12",
    "@graphql-mesh/transform-filter-schema": "^0.2.12",
    "@graphql-mesh/transform-snapshot": "^0.2.12",
    "@magento/peregrine": "^6.0.0",
    "@magento/venia-ui": "^3.0.0",
    "@material-ui/core": "^4.11.0",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/lab": "^4.0.0-alpha.56",
    "@material-ui/styles": "^4.10.0",
    "@n1ru4l/graphql-codegen-relay-optimizer-plugin": "^5.0.0-alpha.0",
    "@next/bundle-analyzer": "^9.4.4",
    "apollo-cache": "^1.3.5",
    "apollo-cache-inmemory": "^1.6.6",
    "apollo-cache-persist": "^0.1.1",
    "apollo-client": "2.6.10",
    "apollo-link": "^1.2.14",
    "apollo-link-context": "^1.0.20",
    "apollo-link-error": "^1.1.13",
    "apollo-link-http": "^1.5.17",
    "apollo-link-retry": "^2.2.16",
    "apollo-link-schema": "^1.2.5",
    "apollo-server-micro": "^2.15.1",
    "apollo-utilities": "^1.3.3",
    "braintree-web-drop-in": "~1.22.1",
    "clone-deep": "^4.0.1",
    "clsx": "^1.1.1",
    "dotenv": "^8.2.0",
    "graphql": "^15.2.0",
    "graphql-tag": "^2.10.1",
    "history": "^4.10.1",
    "informed": "~2.11.17",
    "intl": "^1.2.5",
    "isomorphic-unfetch": "^3.0.0",
    "lodash": "^4.17.15",
    "next": "9.4.4",
    "next-images": "^1.4.0",
    "next-pwa": "^3.0.1",
    "now": "^19.1.1",
    "react": "~16.13.1",
    "react-dom": "~16.13.1",
    "react-feather": "^2.0.8",
    "react-hook-form": "^5.7.1",
    "react-redux": "~7.2.0",
    "react-router-dom": "~5.2.0",
    "react-schemaorg": "^1.1.0",
    "react-spring": "^8.0.27",
    "react-test-renderer": "~16.13.1",
    "redux": "^4.0.5",
    "redux-actions": "^2.6.5",
    "resize-observer": "^1.0.0",
    "schema-dts": "^0.6.0",
    "sitemap": "^6.1.7",
    "ts-node": "^8.10.2",
    "tsconfig-paths": "^3.9.0",
    "type-fest": "^0.16.0",
    "use-resize-observer": "^6.1.0"
  },
  "devDependencies": {
    "@babel/compat-data": "^7.10.4",
    "@babel/core": "^7.10.4",
    "@commitlint/cli": "^9.0.1",
    "@commitlint/config-conventional": "^9.0.1",
    "@graphql-codegen/cli": "^1.16.0",
    "@graphql-codegen/fragment-matcher": "1.16.0",
    "@graphql-codegen/introspection": "1.16.0",
    "@graphql-codegen/schema-ast": "^1.16.0",
    "@graphql-codegen/typescript": "1.16.0",
    "@graphql-codegen/typescript-document-nodes": "1.16.0",
    "@graphql-codegen/typescript-operations": "1.16.0",
    "@graphql-codegen/typescript-react-apollo": "1.16.0",
    "@harelpls/storybook-addon-materialui": "^2.0.0",
    "@magento/pwa-buildpack": "^5.1.1",
    "@semantic-release/git": "^9.0.0",
    "@storybook/addon-actions": "^5.3.19",
    "@storybook/addon-info": "^5.3.19",
    "@storybook/addon-knobs": "^5.3.19",
    "@storybook/addon-links": "^5.3.19",
    "@storybook/addon-storysource": "^5.3.19",
    "@storybook/addons": "^5.3.19",
    "@storybook/core": "^5.3.19",
    "@storybook/preset-typescript": "^3.0.0",
    "@storybook/react": "^5.3.19",
    "@storybook/source-loader": "^5.3.19",
    "@storybook/storybook-deployer": "^2.8.6",
    "@types/clone-deep": "^4.0.1",
    "@types/lodash": "^4.14.157",
    "@types/node": "^14.0.14",
    "@types/react": "^16.9.41",
    "@types/react-redux": "^7.1.9",
    "@types/react-router-dom": "~5.1.0",
    "@types/resize-observer-browser": "^0.1.3",
    "@types/use-resize-observer": "^6.0.0",
    "@types/webpack": "^4.41.18",
    "@typescript-eslint/eslint-plugin": "^3.5.0",
    "@typescript-eslint/parser": "^3.5.0",
    "babel-loader": "^8.1.0",
    "concurrently": "^5.2.0",
    "eslint": "^7.3.1",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-airbnb-typescript": "^8.0.2",
    "eslint-config-prettier": "^6.11.0",
    "eslint-import-resolver-webpack": "^0.12.2",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-react": "^7.20.3",
    "eslint-plugin-react-hooks": "^4.0.7",
    "husky": "^4.2.5",
    "next-plugin-yaml": "^1.0.1",
    "prettier": "^2.0.5",
    "pretty-quick": "^2.0.1",
    "react-to-typescript-definitions": "^2.0.0",
    "semantic-release": "^17.1.1",
    "semantic-release-slack-bot": "^1.6.2",
    "typescript": "^3.9.6",
    "webpack": "^4.43.0"
  },
  "prettier": {
    "trailingComma": "all",
    "tabWidth": 2,
    "semi": false,
    "singleQuote": true,
    "jsxSingleQuote": true,
    "printWidth": 100,
    "proseWrap": "always",
    "overrides": [
      {
        "files": "*.md",
        "options": {
          "printWidth": 80
        }
      }
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "pretty-quick --staged"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ho-nl/m2-pwa.git"
  },
  "release": {
    "plugins": [
      "@semantic-release/commit-analyzer",
      [
        "@semantic-release/release-notes-generator",
        {
          "preset": "conventionalcommits",
          "presetConfig": {
            "types": [
              {
                "type": "docs",
                "section": "Documentation"
              },
              {
                "type": "feat",
                "section": "Features"
              },
              {
                "type": "fix",
                "section": "Fixes"
              },
              {
                "type": "perf",
                "section": "Performance Improvements"
              },
              {
                "type": "build",
                "section": "Build"
              },
              {
                "type": "refactor",
                "section": "Refactor"
              }
            ]
          }
        }
      ],
      "@semantic-release/npm",
      "@semantic-release/git",
      "@semantic-release/github",
      [
        "semantic-release-slack-bot",
        {
          "notifyOnSuccess": true,
          "notifyOnFail": true,
          "markdownReleaseNotes": true,
          "onSuccessTemplate": {
            "blocks": [
              {
                "type": "section",
                "text": {
                  "type": "mrkdwn",
                  "text": "*New release for $repo_url ðŸŽ‰*\n$release_notes"
                }
              }
            ]
          }
        }
      ]
    ],
    "branches": [
      {
        "name": "master"
      }
    ]
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "publishConfig": {
    "access": "restricted"
  }
}
