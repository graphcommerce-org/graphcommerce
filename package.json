{
  "name": "project-reachdigital.nl",
  "homepage": "https://project-reachdigital-nl.now.sh/",
  "version": "1.43.0",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "engines": {
    "node": "12.x"
  },
  "scripts": {
    "postinstall": "patch-package && patch-typed-document-node",
    "dev:api": "yarn pm2 start -s --name api \"NODE_TLS_REJECT_UNAUTHORIZED=0 yarn workspace @reachdigital/soxbase-api dev \" && sleep 5",
    "dev:codegen": "yarn pm2 start -s --name codegen \"CHOKIDAR_USEPOLLING=0 yarn node -r dotenv/config node_modules/.bin/graphql-codegen -w \"",
    "dev:next": "yarn pm2 start -s --name next \"NODE_TLS_REJECT_UNAUTHORIZED=0 yarn next dev\"",
    "dev:log": "yarn pm2 monit",
    "dev": "yarn dev:api && yarn dev:codegen && yarn dev:next && yarn dev:log",
    "postdev": "yarn stop",
    "build:api-start": "yarn workspace @reachdigital/soxbase-api build && yarn pm2 start -s --name api \"NODE_TLS_REJECT_UNAUTHORIZED=0 yarn workspace @reachdigital/soxbase-api start\" && sleep 2",
    "build:codegen": "node -r dotenv/config node_modules/.bin/graphql-codegen",
    "build:next": "next build",
    "build:lighthouse": "ts-node -O '{\"module\":\"commonjs\"}' --transpile-only lib/lighthouse.ts",
    "build": "yarn build:api-start && yarn build:codegen && yarn build:next && yarn build:lighthouse",
    "postbuild": "yarn stop",
    "stop": "yarn pm2 delete -s all",
    "start": "next start",
    "eslint:lint": "eslint --ignore-path .gitignore '**/{*.js,*.ts,*.tsx}'",
    "eslint:fix": "eslint --fix --ignore-path .gitignore '**/{*.js,*.ts,*.tsx}'",
    "prettier:fix": "yarn prettier --ignore-path .gitignore --write '**/*.{ts,tsx,js,css,html}'"
  },
  "sideEffects": false,
  "dependencies": {
    "@adobe/apollo-link-mutation-queue": "^1.0.2",
    "@apollo/client": "^3.2.5",
    "@graphql-codegen/import-types-preset": "^1.18.0",
    "@graphql-mesh/cache-file": "^0.4.17",
    "@graphql-mesh/cli": "^0.11.6",
    "@graphql-mesh/config": "^0.10.12",
    "@graphql-mesh/graphql": "^0.7.5",
    "@graphql-mesh/merger-stitching": "^0.6.16",
    "@graphql-mesh/runtime": "^0.7.11",
    "@graphql-mesh/transform-cache": "^0.7.9",
    "@graphql-mesh/transform-filter-schema": "^0.6.10",
    "@graphql-typed-document-node/core": "^3.1.0",
    "@material-ui/core": "^4.11.0",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/lab": "^4.0.0-alpha.56",
    "@material-ui/styles": "^4.10.0",
    "@n1ru4l/graphql-codegen-relay-optimizer-plugin": "^5.0.0-alpha.0",
    "@next/bundle-analyzer": "^9.5.5",
    "apollo-cache-persist": "^0.1.1",
    "apollo-server-micro": "^2.18.2",
    "clone-deep": "^4.0.1",
    "clsx": "^1.1.1",
    "dotenv": "^8.2.0",
    "framer-motion": "^2.9.1",
    "graphql": "^15.3.0",
    "graphql-tag": "^2.11.0",
    "intl": "^1.2.5",
    "next": "9.5.5",
    "next-images": "^1.6.0",
    "next-pwa": "^3.1.5",
    "next-transpile-modules": "^4.1.0",
    "node-fetch": "^2.6.1",
    "patch-package": "^6.2.2",
    "pm2": "^4.5.0",
    "postinstall-postinstall": "^2.1.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-focus-lock": "^2.4.1",
    "react-hook-form": "^6.9.5",
    "react-schemaorg": "^1.2.0",
    "react-spring": "^8.0.27",
    "resize-observer": "^1.0.0",
    "schema-dts": "^0.7.1",
    "sharp": "^0.26.2",
    "sitemap": "^6.3.2",
    "ts-node": "^9.0.0",
    "tsconfig-paths": "^3.9.0",
    "type-fest": "^0.18.0",
    "use-resize-observer": "^6.1.0",
    "vercel": "^20.1.2"
  },
  "devDependencies": {
    "@babel/compat-data": "^7.12.1",
    "@babel/core": "^7.12.3",
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@graphql-codegen/cli": "^1.18.0",
    "@graphql-codegen/fragment-matcher": "1.17.8",
    "@graphql-codegen/introspection": "1.18.0",
    "@graphql-codegen/schema-ast": "^1.17.8",
    "@graphql-codegen/typed-document-node": "^1.17.9",
    "@graphql-codegen/typescript": "^1.17.11",
    "@graphql-codegen/typescript-document-nodes": "1.17.8",
    "@graphql-codegen/typescript-operations": "1.17.8",
    "@graphql-typed-document-node/patch-cli": "^3.0.8",
    "@semantic-release/git": "^9.0.0",
    "@types/clone-deep": "^4.0.1",
    "@types/lodash": "^4.14.162",
    "@types/node": "^14.14.2",
    "@types/react": "^16.9.53",
    "@types/resize-observer-browser": "^0.1.4",
    "@types/sharp": "^0.26.0",
    "@types/use-resize-observer": "^6.0.0",
    "@types/webpack": "^4.41.23",
    "@typescript-eslint/eslint-plugin": "^4.5.0",
    "@typescript-eslint/parser": "^4.5.0",
    "concurrently": "^5.3.0",
    "eslint": "^7.12.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-airbnb-typescript": "^12.0.0",
    "eslint-config-prettier": "^6.14.0",
    "eslint-import-resolver-webpack": "^0.13.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^4.3.0",
    "prettier": "^2.1.2",
    "pretty-quick": "^3.1.0",
    "semantic-release": "^17.2.1",
    "semantic-release-slack-bot": "^1.6.2",
    "typescript": "^4.0.3",
    "webpack": "^5.1.3"
  },
  "prettier": {
    "trailingComma": "all",
    "tabWidth": 2,
    "semi": false,
    "singleQuote": true,
    "jsxSingleQuote": true,
    "printWidth": 100,
    "proseWrap": "always",
    "overrides": [
      {
        "files": "*.md",
        "options": {
          "printWidth": 80
        }
      }
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "pretty-quick --staged"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ho-nl/m2-pwa.git"
  },
  "release": {
    "plugins": [
      "@semantic-release/commit-analyzer",
      [
        "@semantic-release/release-notes-generator",
        {
          "preset": "conventionalcommits",
          "presetConfig": {
            "types": [
              {
                "type": "docs",
                "section": "Documentation"
              },
              {
                "type": "feat",
                "section": "Features"
              },
              {
                "type": "fix",
                "section": "Fixes"
              },
              {
                "type": "perf",
                "section": "Performance Improvements"
              },
              {
                "type": "build",
                "section": "Build"
              },
              {
                "type": "refactor",
                "section": "Refactor"
              }
            ]
          }
        }
      ],
      "@semantic-release/npm",
      "@semantic-release/git",
      "@semantic-release/github",
      [
        "semantic-release-slack-bot",
        {
          "notifyOnSuccess": true,
          "notifyOnFail": true,
          "markdownReleaseNotes": true,
          "onSuccessTemplate": {
            "blocks": [
              {
                "type": "section",
                "text": {
                  "type": "mrkdwn",
                  "text": "*New release for $repo_url ðŸŽ‰*\n$release_notes"
                }
              }
            ]
          }
        }
      ]
    ],
    "branches": [
      {
        "name": "master"
      }
    ]
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "publishConfig": {
    "access": "restricted"
  }
}
